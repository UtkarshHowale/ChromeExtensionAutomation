<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Smoke Test Cases">
	<listeners>
		<listener class-name="Utils.Listners"></listener>
	</listeners>
	<test name="Smoke Tests Suite Executions">
		<groups>

			<run>
				<include name="Smoke"></include>
			</run>
		</groups>
		<classes>
			<class name="TestCases.OptionPageTestCases" />
			<class name="TestCases.DashBoardPageTestCases" />
		</classes>
	</test>
</suite>